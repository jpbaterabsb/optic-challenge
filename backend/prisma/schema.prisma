// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Voter {
  address String @id
  dateOfBirth DateTime
  email String
  error String?
  txHash String?
}

model VotingResult {
  option String @id
  name String
}

enum State {
  PENDING 
  COMMITTED 
  ABORTED
}